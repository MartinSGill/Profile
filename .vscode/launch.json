  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Module Interactive Session",
      "type": "PowerShell",
      "request": "launch",
      "args": ["-NoProfile"],
      "script": "; cd ${workspaceFolder}; $DebugPreference = 'Continue'; $VerbosePreference = 'Continue; ./build.ps1 -Verbose; Import-Module ./artifacts/MartinsProfile"
    },
    {
      "name": "PowerShell: Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "cwd": "${file}"
    }
  ]
}
